{
  "project_name": "Mental Wellness Coach",
  "version": "1.0.0",
  "description": "AI-powered mental wellness support system using specialized uAgents",
  "agents": [
    {
      "name": "mood_tracker",
      "display_name": "Mental Wellness Mood Tracker",
      "description": "Specialized agent for mood tracking, pattern analysis, and crisis detection",
      "file": "agentverse_mood_tracker.py",
      "capabilities": [
        "mood_tracking",
        "pattern_analysis",
        "crisis_detection",
        "wellness_recommendations"
      ],
      "category": "mental_health",
      "tags": ["mental-health", "mood-tracking", "wellness", "ai-therapy"],
      "price": 0,
      "is_public": true,
      "metadata": {
        "seed_phrase": "mood_tracker_wellness_coach_seed_phrase_here",
        "port": 8000,
        "interval_seconds": 30,
        "crisis_detection_enabled": true,
        "data_retention_days": 30
      }
    },
    {
      "name": "conversation_coordinator",
      "display_name": "Mental Wellness Conversation Coordinator", 
      "description": "AI conversation coordinator providing empathetic support and crisis intervention",
      "file": "agentverse_conversation_coordinator.py",
      "capabilities": [
        "conversation_management",
        "empathetic_responses",
        "crisis_intervention",
        "agent_coordination"
      ],
      "category": "mental_health",
      "tags": ["mental-health", "conversation", "ai-therapy", "crisis-support"],
      "price": 0,
      "is_public": true,
      "metadata": {
        "seed_phrase": "conversation_coordinator_wellness_coach_seed_phrase_here",
        "port": 8001,
        "interval_seconds": 10,
        "crisis_keywords_enabled": true,
        "response_time_ms": 1000
      }
    }
  ],
  "communication": {
    "inter_agent_protocols": [
      {
        "from": "conversation_coordinator",
        "to": "mood_tracker",
        "protocol": "mood_assessment",
        "description": "Send mood assessments from conversations for tracking"
      },
      {
        "from": "mood_tracker", 
        "to": "conversation_coordinator",
        "protocol": "mood_alert",
        "description": "Alert coordinator when intervention is needed"
      }
    ]
  },
  "deployment": {
    "platform": "agentverse",
    "environment": "production",
    "funding": {
      "required": true,
      "amount": "1000000000000000000",
      "token": "FET"
    },
    "monitoring": {
      "health_checks": true,
      "performance_metrics": true,
      "error_tracking": true
    }
  },
  "security": {
    "encryption": true,
    "data_privacy": "high",
    "audit_logging": true,
    "compliance": ["HIPAA_aware", "privacy_first"]
  },
  "integration": {
    "external_apis": [
      {
        "name": "crisis_hotlines",
        "description": "Integration with crisis intervention services",
        "endpoint": "https://suicidepreventionlifeline.org/api",
        "required": false
      }
    ],
    "webhooks": [
      {
        "name": "crisis_alert",
        "description": "Webhook for critical crisis situations",
        "endpoint": "https://your-backend.com/api/crisis/alert",
        "required": false
      }
    ]
  }
} 